<!doctype html>
<html lang="en">

<%- include('../includes/head.ejs') %>

<body>
    <!-- WRAPPER -->
    <div id="wrapper">
        <%- include('../includes/navbar.ejs') %>
        <%- include('../includes/sidebar.ejs') %>
        <!-- MAIN -->
        <div class="main">
            <!-- MAIN CONTENT -->
            <div class="main-content">
                <div class="container-fluid">
                    <h3 class="page-title">사진 추가</h3>
                    <div class="panel">
                    <form method="post" action="/photo/add" enctype="multipart/form-data" class="needs-validation"
                        novalidate>
                        <div class="panel-body">
                            <div class="form-group" align="left">
                                <label class="col-sm-2 control-label">제목</label>
                                <div class="col-sm-10">
                                    <input class="form-control" id="title" name="title" type="text"
                                        placeholder="제목을 입력하세요">
                                </div>
                            </div>
                            <div class="form-group" align="left" style="margin-top: 3%;">
                                <label class="col-sm-2 control-label">사진 첨부</label>
                                <div class="col-sm-10">
                                    <input type="file" name="picture" class="form-control-file border">
                                </div>
                            </div>
                            <div class="form-group" align="center">
                                <button type="submit" class="btn btn-primary" id="registerBtn">저장</button>
                                <button type="reset" class="btn btn-warning">초기화</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <!-- END MAIN CONTENT -->
    </div>
    <!-- END MAIN -->
    <div class="clearfix"></div>
    </div>
    <!-- END WRAPPER -->
    <!-- Javascript -->
    <script src="/vendor/jquery/jquery.min.js"></script>
    <script src="/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="/vendor/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="/vendor/chartist/js/chartist.min.js"></script>

</body>